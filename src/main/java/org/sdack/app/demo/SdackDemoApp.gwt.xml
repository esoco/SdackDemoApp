<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN"
  "file:///S:/eclipse/Lemarit/LemaritWebApp/GWT-SDK/gwt-module.dtd">

<module rename-to='SdackDemoApp'>
  <!-- Inherit the GWT application framework.                     -->
  <inherits name='de.esoco.GwtFramework'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
<!--   <inherits name='com.google.gwt.user.theme.clean.Clean'/> -->
<!--   <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
<!--   <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.sdack.app.demo.client.SdackDemoApp'/>

  <!-- The available locales; to switch URL must be appended with -->
  <!-- ?locale=<locale>                                           -->
  <extend-property name="locale" values="en"/>
  <extend-property name="locale" values="de"/>

  <!-- limit compilation to one locale
  <set-property name="locale" value="en"/>
-->
  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
